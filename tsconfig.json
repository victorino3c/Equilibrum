{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["*"],
      "@components/*": ["src/components/*"],
      "@app/*": ["src/app/*"],
      "@assets/*": ["src/assets/*"],
      "@providers/*": ["src/providers/*"],
      "@libs/*": ["src/libs/*"],
      "@types/*": ["src/types/*"],
      "@store/*": ["src/store/*"],
      "@protected/*": ["src/app/(protected)/*"],
      "@onborading/*": ["src/app/(onboarding)/*"],
      "@userInfo/*": ["src/app/(userInfo)/*"],
      "@api/*": ["src/api/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts", "nativewind-env.d.ts"]
}
